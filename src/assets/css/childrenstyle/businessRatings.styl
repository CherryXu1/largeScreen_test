.businessRatings{
    position: absolute;
    width: 100%;
    height: 100%;
    background: url("./../img/businessRatingsBg.png") no-repeat;
    background-size: 100% 100%;
    display: flex;
    .ratings_left{
        display: flex;
        flex: 1;
        flex-direction: column;
        box-sizing: border-box;
        // padding-bottom: 2.5%;
        .live_viewing,.lookback{
            display: flex;
            flex: 1;
        }
        .live_viewing{
            align-items: flex-end;
            justify-content: center;
            box-sizing: border-box;
            .liveWrappe{
                display: flex;
                justify-content: center;
                width: 87%;
                height: 67%;
            }
        }
        .lookback{
            align-items: flex-end;
            justify-content: center;
            .lookbackWrappe{
                display: flex;
                justify-content: center;
                width: 87%;
                height: 79%;
                box-sizing: border-box;
                .rankingList{
                    height: 85%;    
                }
                
            }
        }    
    }
    .ratings_right{
        display: flex;
        flex: 0.95; 
        flex-direction: column;
        .on_demand,.EPG_ratings{
            display: flex;
            flex: 1;
            width: 92%;
        }
        .on_demand{
            align-items: flex-end;
            justify-content: flex-end;
            .demandWrappe{
                display: flex;
                width: 99%;
                height: 67%;
                justify-content: center;    
            }
        }
        .EPG_ratings{
            align-items: flex-end;
            justify-content: flex-end;
            .EPGWrappe{
                display: flex;
                width: 99%;
                height: 79%;
                justify-content: center;
                .rankingList{
                    height: 85%;    
                }
            } 
        }
          
    }
    
}